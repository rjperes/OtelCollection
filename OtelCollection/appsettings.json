{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenTelemetrySettings": {
    "BaseUrl": "http://localhost:4318/",
    "Protocol": "HttpProtobuf",
    "ServiceName": "rjperes-asus",
    "Metrics": [ "Microsoft.AspNetCore.Hosting", "Microsoft.AspNetCore.Server.Kestrel", "forecast_meter" ],
    "Sources": [ "Forecast.ActivitySource" ],
    "Console": true,
    "Prometheus": true
  }
}
